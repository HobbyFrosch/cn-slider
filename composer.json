{
  "name": "hobbyfrosch/cn-slider",
  "description": "Silder für Chor-Nection",
  "type": "typo3-cms-extension",
  "authors": [
    {
      "name": "Ronny Krämer",
      "email": "info@ronnykramer.de",
      "homepage": "https://www.ronnykraemer.de",
      "role": "developer"
    }
  ],
  "require": {
    "php": "~7.4.0 || ~8.0.0 || ~8.1.0",
    "typo3/cms-core": "^11.5.2 || ^12.0@dev",
    "typo3/cms-extbase": "^11.5.2 || ^12.0@dev",
    "typo3/cms-fluid": "^11.5.2 || ^12.0@dev",
    "typo3/cms-frontend": "^11.5.2 || ^12.0@dev",
    "ext-pdo": "*"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "HF\\CNSlider\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "cnslider"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  }
}
